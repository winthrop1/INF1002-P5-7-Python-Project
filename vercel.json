{
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/website/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/website/js/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/website/images/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "VERCEL": "true"
  }
}
